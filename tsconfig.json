{
  "display": "React",
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "ES2015"
    ],
    "baseUrl": ".",
    "module": "ESNext",
    "target": "es6",
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@assets/*": [
        "./public/static/*"
      ],
      "@styled-system/*": [
        "./styled-system/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
